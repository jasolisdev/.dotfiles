; Bottom Bar - Primary {{{

[bar/bottom-transparent]
background     = #0000000a
height = 90
modules-center =  i3

bottom = true

[bar/bottom-primary]
inherit = bar/base
bottom = true

;override-redirect = true
;wm-restack = i3
;width = 3798

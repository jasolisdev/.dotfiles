; Colors {{{

[xcolor]
bg     = #0a0a0a
fg     = #645b4f
fg-alt = #ebdbb2
color1 = #734541
color2 = #5b6048
color3 = #af865a
color4 = #535c5c
color5 = #775759
color6 = #689d6a
color7 = #A89984
color8 = #928374

[color]
;base-bg     = #8c0a0a0a
base-bg     = #202020

base-comp   = #2524230a
glyph-bg    = #3C3835
module-bg   = #3C3835
selected-bg = #dc1a1a1a

text-fg = ${xcolor.color7}
text-fg-alt = ${xcolor.color7}
icon-fg = ${xcolor.color2}
icon-fg-alt = ${xcolor.color2}
bar-fg  = ${xcolor.color8}

red     = #821717
orange  = #824517

;}}}

; Global {{{

[layout]
icon-font = 12

icon-padding = 1
module-padding = 1
i3-module-padding = 2

bar-format = %{T4}%fill%%indicator%%{F#282828}%empty%%{F-}%{T-}
bar-fill-icon = Ô≠≥

[settings]
; Reload upon receiving XCB_RANDR_SCREEN_CHANGE_NOTIFY events
screenchange-reload = true

;}}}

; Base Bar {{{

[bar/base]
monitor = ${env:MONITOR}
;width = 100%
height = 50
;fixed-center = true

background = ${color.base-bg}

foreground = ${color.text-fg-alt}

; Size of under-/overlines
line-size = 1

cursor-click = pointer
cursor-scroll = ns-resize

;}}}

; Fonts {{{

font-0 = Source Code Pro:style=Bold:size=25;6
font-1 = Source Code Pro:style=Bold:size=25;6
font-2 = Source Code Pro:style=Medium:size=32;9
font-3 = mplus Nerd Font:style=Medium:size=26;8
font-4 = Source Code Pro:style=Medium:size=25;5
font-5 = Source Code Pro:style=Medium:size=30;8
font-6 = Source Code Pro:style=Medium:size=25;4
font-7 = Weather Icons:style=Bold:size=25;8
font-8 = Material Design Icons:style=Regular:size=25;7
font-9 = mplus Nerd Font:style=Medium:size=22;3
font-10 = Font Awesome 5 Free:style=Regular:size=10
font-11 = Product Sans:style=Regular:size=25;6
font-12 = Material Design Icons:style=Regular:size=30;7
font-13 = mplus Nerd Font:style=Medium:size=22;6
font-14 = mplus Nerd Font:style=Medium:size=22;6

;}}}

